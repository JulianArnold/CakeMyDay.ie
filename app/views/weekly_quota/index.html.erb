<h1>Listing weekly_quota</h1>

<table>
  <tr>
    <th>Start date</th>
    <th>End date</th>
    <th>Quota</th>
    <th>Created by</th>
    <th>Updated by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @weekly_quota.each do |weekly_quotum| %>
  <tr>
    <td><%= weekly_quotum.start_date %></td>
    <td><%= weekly_quotum.end_date %></td>
    <td><%= weekly_quotum.quota %></td>
    <td><%= weekly_quotum.created_by %></td>
    <td><%= weekly_quotum.updated_by %></td>
    <td><%= link_to 'Show', weekly_quotum %></td>
    <td><%= link_to 'Edit', edit_weekly_quotum_path(weekly_quotum) %></td>
    <td><%= link_to 'Destroy', weekly_quotum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

