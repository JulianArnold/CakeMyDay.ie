<h1>Listing options_lists</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Visible label</th>
    <th>Options</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @options_lists.each do |options_list| %>
  <tr>
    <td><%= options_list.name %></td>
    <td><%= options_list.visible_label %></td>
    <td><%= options_list.options %></td>
    <td><%= link_to 'Show', options_list %></td>
    <td><%= link_to 'Edit', edit_options_list_path(options_list) %></td>
    <td><%= link_to 'Destroy', options_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Options list', new_options_list_path %>
